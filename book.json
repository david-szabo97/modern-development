{
  "title": "Modern Development Handbook",
  "plugins": [
    "edit-link",
    "ga",
    "github",
    "prism",
    "canonical-link",
    "-highlight",
    "-fontsettings"
  ],
  "pdf": {
    "fontSize": 10,
    "paperSize": "a5",
    "margin": {
      "top": 64,
      "bottom": 64,
      "left": 40,
      "right": 40
    }
  },
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/rstacruz/modern-development/edit/master",
      "label": "Edit"
    },
    "ga": {
      "token": "UA-20473929-1"
    },
    "github": {
      "url": "https://github.com/rstacruz/modern-development/"
    },
    "sharing": {
      "facebook": false,
      "twitter": false,
      "google": false,
      "weibo": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "google",
        "twitter",
        "weibo",
        "instapaper"
      ]
    },
    "canonical-link": {
      "baseURL": "https://modern-development.now.sh"
    }
  }
}
